{
  "name": "openclaw-memory",
  "version": "1.1.0",
  "description": "OpenClaw memory management - Search, compress, and navigate AGENTS.md, MEMORY.md, and memory/ directory with security hardening",
  "author": "Pi-Agent",
  "license": "MIT",
  "keywords": ["openclaw", "memory", "search", "compression", "agents", "security"],
  "main": "openclaw-memory.sh",
  "assets": {
    "logo": "assets/icon.png",
    "banner": "assets/banner.png"
  },
  "dependencies": {
    "openclaw": ">=2.0"
  },
  "capabilities": [
    "memory_search",
    "memory_compress",
    "agents_list",
    "memory_stats",
    "security_hardened"
  ],
  "security": {
    "features": [
      "input_validation",
      "command_whitelisting",
      "path_traversal_protection",
      "command_injection_prevention",
      "dos_protection",
      "security_logging"
    ]
  }
}
